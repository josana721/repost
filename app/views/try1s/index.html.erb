<h1>Listing try1s</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Cpnumber</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @try1s.each do |try1| %>
      <tr>
        <td><%= try1.name %></td>
        <td><%= try1.cpnumber %></td>
        <td><%= try1.address %></td>
        <td><%= link_to 'Show', try1 %></td>
        <td><%= link_to 'Edit', edit_try1_path(try1) %></td>
        <td><%= link_to 'Destroy', try1, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Try1', new_try1_path %>
